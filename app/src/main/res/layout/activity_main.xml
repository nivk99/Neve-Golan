<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#8BB6CF"
    android:paddingLeft="16dp"
    android:paddingTop="@dimen/cardview_compat_inset_shadow"
    android:paddingRight="16dp"
    android:paddingBottom="16dp"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView_main_tlv"
        android:layout_width="282dp"
        android:layout_height="90dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.348"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/first_main_picture"
        tools:ignore="ImageContrastCheck" />

    <ImageButton
        android:id="@+id/imageButton_timetable"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:onClick="click_timetable"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.144"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.422"
        app:srcCompat="@drawable/calendar"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView_main_concern"
        android:layout_width="328dp"
        android:layout_height="86dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/second_main_picture" />

    <Button
        android:id="@+id/button_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="112dp"
        android:background="#3F51B5"
        android:backgroundTint="#3F51B5"
        android:onClick="click_logout"
        android:text="logout"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_main_concern"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.474"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView_teacher"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="71dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="172dp"
        android:onClick="click_teachers"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_main_concern"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView_student"
        app:srcCompat="@drawable/teacher"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView_student"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="176dp"
        android:onClick="click_student"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_main_concern"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/student"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView_private"
        android:layout_width="147dp"
        android:layout_height="171dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="16dp"
        android:onClick="click_private"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_neve_golan"
        app:srcCompat="@drawable/personal"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView_student"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginBottom="120dp"
        android:fontFamily="sans-serif-black"
        android:text="Student"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_student"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView_teachers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:layout_marginBottom="124dp"
        android:fontFamily="sans-serif-black"
        android:text="Teachers"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_teacher"
        app:layout_constraintStart_toEndOf="@+id/textView_student" />

    <TextView
        android:id="@+id/textView_timetable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="120dp"
        android:fontFamily="sans-serif-black"
        android:text="Timetable"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_timetable"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView_private"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:layout_marginTop="51dp"
        android:layout_marginBottom="132dp"
        android:fontFamily="sans-serif-black"
        android:text="Private"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_private"
        app:layout_constraintStart_toEndOf="@+id/textView_timetable"
        app:layout_constraintTop_toBottomOf="@+id/textView_neve_golan" />

    <TextView
        android:id="@+id/textView_neve_golan"
        android:layout_width="291dp"
        android:layout_height="64dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="412dp"
        android:fontFamily="cursive"
        android:text="Neve Golan"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_logout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_main_tlv"
        app:layout_constraintVertical_bias="0.246" />

</androidx.constraintlayout.widget.ConstraintLayout>